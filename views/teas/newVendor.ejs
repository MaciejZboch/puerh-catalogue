<% layout('layouts/boilerplate') %>
<form
  action="/tea/newVendor"
  method="post"
  enctype="multipart/form-data"
  novalidate
  class="validated-form"
>
  <input type="text" name="vendor" id="vendor" />
  <input
    type="file"
    id="formFile"
    name="image"
    accept="image/png, image/jpeg, image/jpg"
    multiple
  />
  <button>Submit</button>
</form>
