<% layout('layouts/boilerplate') %>
<div class="text-center">
  <h1 class="mb-5">Add a producer</h1>
  <form action="/tea/newProducer" method="post" enctype="multipart/form-data">
    <label for="producer">Producer's name:</label>
    <input type="text" name="producer" id="producer" />
    <button class="btn btn-success">Submit</button>
  </form>
</div>
